{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "JWT_SECRET": "@jwt_secret",
    "NODE_ENV": "production",
    "FRONTEND_URL": "@frontend_url"
  },
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}

